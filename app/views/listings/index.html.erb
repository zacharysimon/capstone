<div class="dashboard container">
    
    <div class="ibox-content ibox">
         <div class="row">
            <div class="col-sm-9">
                <div class="input-group">
                    <a href='/listings/new'>Create new listing </a>
                </div>
            </div>
            <div class="col-sm-3 wrench">
                <a href="/dashboards/edit">
                    <i class="fa fa-wrench"></i>
                </a>
            </div>
         </div>
    </div>
    <div class="table">
            <table class="table table-striped">
                <thead>
                    <tr> 
                        <th ng-repeat="header in headings">{{header.header_title}}</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="listing in listings">
                        <td ng-repeat="dashboard in listing.dashboard">
                            <a href='listings/{{listing.id}}'>{{dashboard.attribute_value}}</a>
                        </td>
                    </tr>
                </tbody>
            </table>
     </div>
</div>
