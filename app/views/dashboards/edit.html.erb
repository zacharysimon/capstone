<div class="dashboard-container container">
  
        <div class="col-lg-7"></div>
        <div class="dashboard-text col-lg-3 settings">
          <a href='/listings' ng-click="updateDashboard(this, <%= current_user.id %>)"><p>Save Dashboard</p></a>
          <a class="dashboard-save pull-right" href='/listings' ng-click="updateDashboard(this, <%= current_user.id %>)"><i class="fa fa-floppy-o fa-2x"></i></a>
        </div>



  <div class="container simpleDemo">
      <div class="row">
        <div class="col-md-12 dashboard-boxes">
          <div ng-repeat="(listName, list) in models.lists" class="col-md-5">
            <div class="panel panel-info well">
              <div class="panel-heading">
                <h2 class="panel-title"> {{listName}}</h2>
                  <ol dnd-list="list">
                     <li ng-repeat="item in list"
                     dnd-draggable="item"
                     dnd-moved="list.splice($index, 1)"
                     dnd-effect-allowed="move"
                     dnd-selected="models.selected = item"
                     ng-class="{'selected': models.selected === item}">
                     {{item.label}}
                    </li>
                  </ol>
                </div>
              </div>
            </div>
        </div>
    </div>
  </div>

</div>








